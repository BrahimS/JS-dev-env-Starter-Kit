{
  "name": "starter-kit",
  "version": "1.0.0",
  "description": "JS dev envi",
  "main": "index.js",
  "author": "Brahim Sourny",
  "license": "MIT",
  "scripts": {
    "prestart": "babel-node scripts/serverMessage.js",
    "start": "npm-run-all --parallel server lint:watch test:watch",
    "server": "babel-node scripts/server.js",
    "lint": "esw webpack.config.* src scripts --color ",
    "lint:watch": "yarn lint -- --watch",
    "security-check": "nsp chech",
    "tunnel": "lt --port 3000",
    "share": "npm-run-all --parallel server tunnel",
    "prod": "yarn run clean && NODE_ENV=production webpack -p",
    "clean": "rimraf ./build/*",
    "test": "mocha --compilers js:babel-core/register --reporter spec scripts/testSetup.js \"src/test/**/*.js\"",
    "test:watch": "yarn test -- --watch",
    "deploy": "surge ./build"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.0.0",
    "chalk": "^1.1.3",
    "cheerio": "^0.22.0",
    "compression": "^1.6.2",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "eslint-watch": "^3.1.0",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "image-webpack-loader": "^3.3.1",
    "jsdom": "10.1.0",
    "json-schema-faker": "^0.5.0-rc3",
    "json-server": "^0.10.1",
    "localtunnel": "^1.8.2",
    "mocha": "^3.4.2",
    "nock": "^9.0.13",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "nsp": "^2.6.3",
    "numeral": "^2.0.6",
    "open": "^0.0.5",
    "path": "0.12.7",
    "pug-html-loader": "^1.1.4",
    "rimraf": "^2.6.1",
    "sass-lint": "^1.10.2",
    "sass-loader": "^6.0.5",
    "sinon": "^2.3.1",
    "style-loader": "^0.17.0",
    "surge": "^0.19.0",
    "webpack": "^2.5.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-dev-server": "^2.4.5",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-md5-hash": "^0.0.5",
    "whatwg-fetch": "^2.0.3"
  }
